<h1>Listing Users</h1>
<table>
<% @recipes.each do |account| %>
<tr>
<td><%= account.nombre %></td>
<td><%= link_to 'Destroy',account_path(account), method: :delete,data: { confirm: 'Are you sure?' } %></td>
<td><%= link_to 'Edit',edit_account_path(account.id) %></td>
<td><%= link_to 'Show',account_path(account) %></td>
</tr>
<% end %>
</table>
<%= link_to "Crear ", new_account_path %>
